<%= form_for(@user, url: signup_path) do |f| %>

  <%= f.label :full_name %>
  <%= f.text_field :full_name %>

  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit '登録' %>

<% end %>
